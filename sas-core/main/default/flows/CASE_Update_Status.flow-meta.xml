    <decisions>
        <description>It validates the latest case record type and department of the case before making a status update</description>
        <name>Is_the_case_record_type_and_department_valid</name>
        <label>Is the case record type and department valid?</label>
        <locationX>1122</locationX>
        <locationY>458</locationY>
        <defaultConnector>
